{
  "name": "alpha-auth-common",
  "version": "1.0.0",
  "author": "aSH <ash.uncover@gmail.com>",
  "description": "Util project for alpha-auth",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ash-uncover/alpha-auth-common"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "build": "npm run build:generate && npm run build:compile && npm run build:babel && npm run build:bundle",
    "build:babel": "babel src --extensions .ts,.tsx --out-dir build",
    "build:bundle": "webpack",
    "build:compile": "tsc",
    "build:generate": "js-swagger-generator --files=resources/openapi --output=src",
    "format": "prettier --write \"src//*.ts\" \"src//.tsx\" \"src/**/.js\" \"src/*/.jsx\"",
    "install-local": "npm i && npm link @uncover/js-swagger-generator",
    "lint": "tslint -p tsconfig.json",
    "start:all": "npm run install-local && npm run build && concurrently \"npm run start:db\" \"npm run start:api\" \"npm run start:ui\"",
    "start:api": "cd ../alpha-auth-api && npm run install-local && npm run start:dev",
    "start:db": "cd ../alpha-auth-db && LocalStartDB.bat",
    "start:ui": "cd ../alpha-auth && npm run install-local && npm run start:dev"
  },
  "dependencies": {
    "@uncover/js-utils": "0.0.13",
    "@uncover/js-utils-fetch": "0.0.3",
    "@uncover/js-utils-logger": "0.1.5"
  },
  "devDependencies": {
    "@babel/cli": "7.22.10",
    "@babel/core": "7.22.11",
    "@babel/preset-env": "7.22.14",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.11",
    "@types/node": "20.5.8",
    "@uncover/js-swagger-generator": "0.0.8",
    "concurrently": "8.2.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "webpack-cli": "5.1.4"
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "node": true,
      "jest": true
    }
  }
}
