{
  "name": "alpha-auth-common",
  "version": "1.0.0",
  "author": "aSH <ash.uncover@gmail.com>",
  "private": true,
  "main": "build/index.ts",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "npm run build:generate && npm run build:bundle && npm run build:compile",
    "build:bundle": "webpack",
    "build:compile": "tsc",
    "build:generate": "js-swagger-generator --files=resources/swagger --output=src",
    "format": "prettier --write \"src//*.ts\" \"src//.tsx\" \"src/**/.js\" \"src/*/.jsx\"",
    "install-local": "npm i && npm link @uncover/js-swagger-generator",
    "lint": "tslint -p tsconfig.json",
    "start:all": "concurrently \"npm run start:db\" \"npm run start:api\" \"npm run start:ui\"",
    "start:api": "cd ../alpha-auth-api && npm run start:dev",
    "start:db": "cd ../alpha-auth-db && LocalStartDB.bat",
    "start:ui": "cd ../alpha-auth && npm run start:dev"
  },
  "dependencies": {
    "@uncover/js-utils": "0.0.13",
    "@uncover/js-utils-fetch": "0.0.3",
    "@uncover/js-utils-logger": "0.1.5"
  },
  "devDependencies": {
    "@types/node": "20.5.7",
    "@uncover/js-swagger-generator": "0.0.7",
    "concurrently": "8.2.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "webpack-cli": "^5.1.4"
  }
}
